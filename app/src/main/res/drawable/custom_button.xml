<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="30dp" />
            <stroke android:width="1dip" android:color="@android:color/holo_blue_dark" />
            <solid android:width="1dp" android:color="@android:color/holo_blue_bright" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="30dp" />
            <stroke android:width="1dip" android:color="@android:color/holo_blue_dark" />
            <solid android:color="@android:color/holo_blue_light"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="30dp" />
            <stroke android:width="1dp" android:color="@android:color/holo_blue_dark" />
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
</selector>